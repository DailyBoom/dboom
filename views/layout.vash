    </head>
    <body>
    	<div class="top-margin cyan darken-3">
    	</div>
  		<div class="navbar">
  			<nav class="cyan darken-3">
  				<div class="container center">
  					<a href="/" class=""><img class="nav-logo2 left" src="/images/db_logo01.png" /></a>
  					<span id="time-left" class="d-clock hide-on-small-only"></span>
  					<script>
  						$(document).ready(function() {
  						    var interval = setInterval(function() {
  						        var momentNow = moment();
  						        $('#time-left').html(momentNow.format('hh:mm:ss'));
  						        $('#time-left-mobile').html(momentNow.format('hh:mm:ss'));
  						    }, 100);
  						});
  					</script>

  <!--					<a href="/" class="brand-logo">Daily Boom!</a>-->
  					@if (model.user) {
  						<ul class="right hide-on-med-and-down">
  							<li>
  								<!-- Dropdown Trigger -->
  								<a id="btn-profile" class='dropdown-button btn-floating cyan z-depth-1' href='#' data-activates='dropdown-profile'>
  <!--									<img class="img-profile" src="/images/nobody.jpg" alt="" />-->
  									<i class="fa fa-user fa-4x"></i>
  								</a>

  								<!-- Dropdown Structure -->
  								<ul id="dropdown-profile" class="dropdown-content">
  									<li><a class="waves-effect waves-light" href="#!">@model.user.name</a></li>
  									<li class="divider"></li>
  									<li><a class="waves-effect waves-light" href="!#">My Page</a></li>
  									<li><a class="waves-effect waves-light" href="!#">Settings</a></li>
  									<li><a class="waves-effect waves-light" href="/logout">Log Out</a></li>
  								</ul>
  							</li>
  						</ul>
  						<ul id="slide-out" class="side-nav">
  							<li><a class="waves-effect waves-light" href="#!">@model.user.name</a></li>
  							<li><a class="waves-effect waves-light" href="/logout">Log Out</a></li>
  						</ul>
  					}
  					else {
  						<ul class="right hide-on-med-and-down">
  							<li><a class="waves-effect waves-light" href="/login">Login</a></li>
  							<li><a class="waves-effect waves-light" href="#!">Signup</a></li>
  						</ul>
  						<ul id="slide-out" class="side-nav">
  							<li><a class="waves-effect waves-light" href="/login">Login</a></li>
  							<li><a class="waves-effect waves-light" href="#!">Signup</a></li>
  						</ul>
  					}
  					<a href="#" data-activates="slide-out" class="button-collapse right">
  						<i class="mdi-navigation-menu"></i>
  					</a>
  				</div>
  			</nav>
  		</div>
  		<script>
  			$('.dropdown-button').dropdown({
  					inDuration: 300,
  					outDuration: 225,
  					constrain_width: false, // Does not change width of dropdown to that of the activator
  					hover: false, // Activate on hover
  					gutter: 0, // Spacing from edge
  					belowOrigin: true, // Displays dropdown below the button
  					alignment: 'left' // Displays dropdown with edge aligned to the left of button
  				}
  			);
  		</script>
        @html.include("error", model)
        @html.block("body")
  		<footer class="page-footer cyan darken-3 center">
  			<div class="container">
  				<p class="grey-text text-lighten-4">
  					<span>
  						<a class="grey-text text-lighten-4" href="#"><img class="social" src="/images/facebook.png" /></a>
  						&nbsp;
  						<a class="grey-text text-lighten-4" href="#"><img class="social" src="/images/kakaostory_logo.png" /></a>
  					</span>
  				</p>
  			</div>
  			<div class="footer-copyright">
  				<div class="container">
  					Â© 2015 Daily Boom. All rights reserved.
  					<a class="grey-text text-lighten-4 right" href="#!"></a>
  				</div>
  			</div>
  			<div class="hide-on-med-and-up">
  				<p class="section-blank"></p>
  			</div>
  		</footer>
    </body>
</html>
