  					<span id="time-left-label" class="d-clock navbar-align clock-font">남은시간</span>
            <span id="time-left" class="d-clock navbar-align"></span>
  					<script>
            $(window).load(function() {
                $('#time-left-label').fadeOut(2500, function(){
                  var momentNow = moment();
                  $('#time-left').hide();
                  $('#time-left').html(momentNow.format('hh:mm:ss'));
                  $('#time-left').fadeIn(1000);
                  var interval = setInterval(function() {
                      var momentNow = moment();
                      $('#time-left').html(momentNow.format('hh:mm:ss'));
                  }, 100);
                });
  						});
  					</script>

