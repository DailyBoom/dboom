@html.extend("layout", function(model) { 
	@html.block("body", function(model) {
		<script>
			$('.button-collapse').sideNav({
					menuWidth: 240, // Default is 240
					edge: 'right', // Choose the horizontal origin
					closeOnClick: true
				}
			);
		</script>
		
<!-- Share Modal Structure 
		<div id="modal-share" class="modal cyan darken-3 bottom-sheet center">
			<div class="modal-content">
				<span class="share-social">
					<a class="social-link white-text" href="{BRAND HOMEPAGE URL}" target="_blank"><i class="fa fa-globe fa-4x"></i></a>
					<a class="social-link white-text" href="{BRAND FACEBOOK URL}" target="_blank"><i class="fa fa-facebook-official fa-4x"></i></a>
					<a class="social-link white-text" href="{BRAND TWITTER URL}" target="_blank"><i class="fa fa-twitter fa-4x"></i></a>
